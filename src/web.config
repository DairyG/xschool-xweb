<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="login.html" />
            </files>
        </defaultDocument>
        <security>
            <requestFiltering>
                <requestLimits maxAllowedContentLength="104857600" maxUrl="104857600" maxQueryString="2048152" />
            </requestFiltering>
        </security>
    </system.webServer>
</configuration>
